<div class="container mt-3">
 <h1>Edit your vehicle: <%= @vehicle.category %></h1>
 <%= simple_form_for @vehicle do |f| %>
  <%= f.input :category, :collection => ["Bicycle", "E-bike", "Scooter", "Wheel Chair", "Skate", "Rollers"], label: "Edit your vehicle category" %>
  <%= f.input :title %>
  <%= f.input :location %>
  <%= f.input :price %>
  <%= f.input :image_url, as: :file %>
  <%= f.input :description %>
  <%= f.submit %>
 <% end %>
</div>
